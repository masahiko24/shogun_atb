<div class="postin">

    <div class="page-top">
     <%= form_with model: @photo, local: true do |f| %>

     <div class="postin-form-left">
     <%= render "devise/shared/error_messages", resource: resource %>

    <div class="field">

      <%= f.label :Nickname, "ニックネーム" %><br />
      <%= f.text_field :nickname %>
      </div>

    <div class="field">
      <%= f.label :Title, "タイトル" %><br />
      <%= f.text_field :title %>
    </div>

    <div class="field">
      <%= f.label :Image, "画像" %><br />
      <%= f.file_field :image %>
    </div>

    <div class="field">
      <%= f.label :Memo, "メモ"%><br />
      <%= f.text_area :memo, rows: "3" %>
    </div>

    <div class="actions">
      <%= f.submit "SEND", class: :form__btn%>
    </div>

    <% end %>
    </div>
    </div>

        <div class="page-top">
        <div class="postin-form">
    </div>
    </div>
  </div>
</div>